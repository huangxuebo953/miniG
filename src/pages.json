{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^fui-(.*)": "firstui-uni/firstui/fui-$1/fui-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/stu/login",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/stu/school",
      "style": {
        "navigationBarTitleText": "选择学校"
      }
    },
    {
      "path": "pages/stu/bind",
      "style": {
        "navigationBarTitleText": "身份绑定"
      }
    },
    {
      "path": "pages/stu/bindManager",
      "style": {
        "navigationBarTitleText": "学生绑定管理"
      }
    },
    {
      "path": "pages/stu/consumer",
      "style": {
        "navigationBarTitleText": "消费记录"
      }
    },
    {
      "path": "pages/stu/test",
      "style": {
        "navigationBarTitleText": "测试页"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
